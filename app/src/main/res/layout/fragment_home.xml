<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/estilodeapp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <TextView
        android:id="@+id/tvHomePrueba2"
        style="@style/estilodeapp"
        android:layout_width="329dp"
        android:layout_height="80dp"
        android:layout_marginStart="161dp"
        android:layout_marginEnd="193dp"
        android:text="@string/mensaje_home"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.807" />

    <TextView
        android:id="@+id/tvHomePrueba"
        style="@style/estilodeapp"
        android:layout_width="207dp"
        android:layout_height="54dp"
        android:layout_marginStart="161dp"
        android:layout_marginEnd="193dp"
        android:text="@string/app_name"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.393"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="285dp"
        android:layout_height="321dp"
        android:src="@mipmap/ic_imagen_home"
        app:layout_constraintBottom_toTopOf="@+id/tvHomePrueba2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvHomePrueba"
        app:layout_constraintVertical_bias="0.171"
        app:srcCompat="@mipmap/ic_imagen_home"
        tools:srcCompat="@mipmap/ic_imagen_home" />
</androidx.constraintlayout.widget.ConstraintLayout>